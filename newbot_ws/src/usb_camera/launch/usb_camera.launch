<?xml version="1.0"?>
<launch>

  <node pkg="usb_camera" type="usb_camera" name="usb_camera" output="screen">
 	  
 	  <param name="pub_image_topic" value="/image_raw/compressed" type="str" />
 	  
	  <param name="dev_name" value="/dev/video0" type="str" />

	  <param name="frame_id" value="camera_link" type="str" />
	  
	  <param name="width" value="1280" type="int" />
	  
	  <param name="height" value="720" type="int" />
	  
	  <param name="div" value="1" type="int" />
	  
  </node>
  
</launch>
