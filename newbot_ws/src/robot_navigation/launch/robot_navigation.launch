<?xml version="1.0"?>
<launch>
    <node pkg="map_server" name="map_server" type="map_server" args="$(find robot_navigation)/maps/map.yaml">
      <param name="frame_id" value="map"/>
    </node>
    
      <!--<node pkg="tf" type="static_transform_publisher" name="odom_map_broadcaster" args="0 0 0 0 0 0 /map /odom 50" />  xxx ms-->
  
    
     <node pkg="amcl" type="amcl" name="amcl" output="screen">
      <rosparam file="$(find robot_navigation)/config/robot/amcl_params.yaml" command="load" />
      <param name="initial_pose_x"            value="0.0"/>
      <param name="initial_pose_y"            value="0.0"/>
      <param name="initial_pose_a"            value="0.0"/>
    </node>

    
    <include file="$(find robot_navigation)/launch/move_base.launch" /> 
    
</launch>
